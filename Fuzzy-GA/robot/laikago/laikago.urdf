<?xml version="1.0" ?>
<!-- Copiado y adaptado de https://github.com/erwincoumans/motion_imitation/tree/master/motion_imitation/robots/laikago_description/urdf -->
<robot name="laikago">
  <link name="trunk">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.3"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.6 0.2 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.6 0.2 0.1"/>
      </geometry>
    </collision>
  </link>
  <!-- Solo se muestra una pata como ejemplo, el modelo real tiene 4 patas con 3 DOF cada una -->
  <link name="fl_upper_leg">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.02"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.02"/>
      </geometry>
    </collision>
  </link>
  <joint name="fl_hip_joint" type="revolute">
    <parent link="trunk"/>
    <child link="fl_upper_leg"/>
    <origin xyz="0.3 0.1 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="5" velocity="5"/>
  </joint>
  <!-- ... (agregar el resto de las patas y articulaciones segÃºn el URDF original) ... -->
</robot>

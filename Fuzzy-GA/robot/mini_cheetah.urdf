<?xml version="1.0"?>
<robot name="mini_cheetah">
  <!-- Cuerpo principal -->
  <link name="base">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.06"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.3 0.1 0.05"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>

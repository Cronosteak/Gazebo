    </collision>
  </link>
  <joint name="fl_hip_joint" type="revolute">
    <parent link="base"/>
    <child link="fl_hip"/>
    <origin xyz="0.15 0.05 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="2" velocity="2"/>
